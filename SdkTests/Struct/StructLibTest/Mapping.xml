<?xml version="1.0" encoding="utf-8"?>
<config id="structlib-test" xmlns="urn:SharpGen.Config">
  <namespace>StructLibTest</namespace>
  <assembly>StructLibTest</assembly>

  <sdk name="StdLib" />
  
  <include-dir>$(THIS_CONFIG_PATH)\..\StructLib</include-dir>
  <include file="StructLib.h">
    <attach>SimpleStruct</attach>
    <attach>GetSimpleStruct</attach>
  </include>

  <extension>
    <create class="StructLibTest.Functions" visibility="public static" />
  </extension>

  <bindings>
  </bindings>

  <mapping>
    <map function="Get.*" dll='"StructLib.dll"' group="StructLibTest.Functions" />
    <!-- <map function="And" dll='"StructLib.dll"' group="StructLibTest.Functions" /> -->
    <!-- <map field="IntToBoolArray::i" type="bool" /> -->
  </mapping>

  <depends>SharpGen.Runtime</depends>
</config>